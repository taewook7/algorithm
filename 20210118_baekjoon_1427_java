import java.util.Arrays;
import java.util.Scanner;

public class Main {
	public static void main(String[] args){
        
    Scanner sc =new Scanner(System.in);
        
  
    String s = sc.nextLine();
    int n = s.length();
    int[] value = new int[n];
        
    for(int i=0; i<n; i++) 
        value[i] = s.charAt(i) -48;
    Arrays.sort(value);
        
    for(int i=n-1; i>=0; i--) 
        System.out.print(value[i]);
    }
}
    
    